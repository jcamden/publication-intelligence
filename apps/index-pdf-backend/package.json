{
	"name": "@pubint/index-pdf-backend",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"exports": {
		".": {
			"types": "./src/routers/index.ts",
			"default": "./src/routers/index.ts"
		}
	},
	"scripts": {
		"dev": "tsx watch src/index.ts",
		"build": "tsc",
		"start": "node dist/index.js",
		"clean": "rm -rf dist",
		"typecheck": "tsc --noEmit",
		"test:reset-db": "cd ../../db/gel && ./reset-test-branch.sh",
		"test": "pnpm test:reset-db && NODE_ENV=test vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:integration": "vitest run --testNamePattern='Integration'",
		"test:service": "vitest run --testNamePattern='Service'"
	},
	"dependencies": {
		"@fastify/cors": "^11.2.0",
		"@pubint/core": "workspace:*",
		"@pubint/events": "workspace:*",
		"@pubint/llm": "workspace:*",
		"@pubint/pdf": "workspace:*",
		"@trpc/server": "^11.8.1",
		"dotenv": "^17.2.3",
		"fastify": "^5.7.1",
		"gel": "^2.2.0",
		"pino": "^9.14.0",
		"pino-pretty": "^13.0.0",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@types/node": "^22.10.5",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3"
	}
}
