{
	"name": "@pubint/index-pdf-frontend",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"clean": "rm -rf .next",
		"typecheck": "tsc --noEmit",
		"test": "pnpm test:unit && pnpm test:interaction",
		"test:unit": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"test:interaction": "vitest run --config=vitest.storybook.config.ts; VITEST_EXIT=$?; tsx ../../scripts/format-interaction-test-results.ts test-results/interaction-tests.json test-results/interaction-summary.md; exit $VITEST_EXIT",
		"test:interaction:watch": "vitest --config=vitest.storybook.config.ts --watch",
		"storybook": "storybook dev -p 6006",
		"storybook:vrt": "storybook dev -p 6006 --config-dir .storybook-vrt",
		"build-storybook": "storybook build",
		"test:e2e": "playwright test --config=playwright/e2e/e2e.config.ts",
		"test:e2e:ui": "playwright test --config=playwright/e2e/e2e.config.ts --ui"
	},
	"dependencies": {
		"@base-ui/react": "^1.1.0",
		"@hello-pangea/dnd": "^18.0.1",
		"@hookform/resolvers": "^5.2.2",
		"@pubint/core": "workspace:*",
		"@pubint/llm": "workspace:*",
		"@pubint/pdf": "workspace:*",
		"@pubint/storybook-config": "workspace:*",
		"@pubint/yaboujee": "workspace:*",
		"@tanstack/react-form": "^1.28.0",
		"@tanstack/react-query": "^5.90.20",
		"@tanstack/zod-form-adapter": "^0.42.1",
		"@trpc/client": "^11.8.1",
		"@trpc/react-query": "^11.8.1",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"jotai": "^2.17.0",
		"lucide-react": "^0.563.0",
		"next": "^15.1.6",
		"pdfjs-dist": "^4.10.38",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.4.0",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@playwright/test": "^1.58.0",
		"@pubint/index-pdf-backend": "workspace:*",
		"@pubint/vitest-config": "workspace:*",
		"@storybook/addon-a11y": "^10.2.0",
		"@storybook/addon-docs": "^10.2.0",
		"@storybook/addon-vitest": "^10.2.0",
		"@storybook/nextjs-vite": "^10.2.0",
		"@storybook/react": "^10.2.0",
		"@storybook/test": "^8.6.15",
		"@tailwindcss/postcss": "^4.1.18",
		"@tailwindcss/vite": "^4.1.18",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.2",
		"@types/node": "^22.10.5",
		"@types/react": "^19.2.9",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.2",
		"@vitest/browser-playwright": "^4.0.18",
		"@vitest/coverage-v8": "^4.0.18",
		"autoprefixer": "^10.4.23",
		"jsdom": "^27.4.0",
		"playwright": "^1.58.0",
		"postcss": "^8.5.6",
		"storybook": "^10.2.0",
