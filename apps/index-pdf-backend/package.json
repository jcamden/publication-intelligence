{
	"name": "@pubint/index-pdf-backend",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"exports": {
		".": {
			"types": "./src/routers/index.ts",
			"default": "./src/routers/index.ts"
		}
	},
	"scripts": {
		"dev": "tsx watch src/index.ts",
		"build": "tsc",
		"start": "node dist/index.js",
		"lint:biome": "biome ci .",
		"clean": "rm -rf dist",
		"typecheck": "tsc --noEmit",
		"test": "pnpm test:unit",
		"test:unit": "NODE_ENV=test vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:integration": "vitest run --testNamePattern='Integration'",
		"test:service": "vitest run --testNamePattern='Service'"
	},
	"dependencies": {
		"@fastify/cors": "^11.2.0",
		"@fastify/multipart": "^9.4.0",
		"@pubint/core": "workspace:*",
		"@pubint/events": "workspace:*",
		"@pubint/llm": "workspace:*",
		"@pubint/pdf": "workspace:*",
		"@trpc/server": "^11.8.1",
		"bcryptjs": "^2.4.3",
		"dotenv": "^17.2.3",
		"drizzle-orm": "^0.39.3",
		"fastify": "^5.7.1",
		"jsonwebtoken": "^9.0.2",
		"pino": "^9.14.0",
		"pino-pretty": "^13.0.0",
		"postgres": "^3.4.5",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@pubint/vitest-config": "workspace:*",
		"@types/bcryptjs": "^2.4.6",
		"@types/jsonwebtoken": "^9.0.7",
		"@types/node": "^22.10.5",
		"drizzle-kit": "^0.30.1",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3"
	}
}
