		"@types/react": "^19.0.6",
		"@types/react-dom": "^19.0.2",
		"@vitest/browser-playwright": "^4.0.18",
		"@vitest/coverage-v8": "^4.0.18",
		"autoprefixer": "^10.4.23",
		"playwright": "^1.58.0",
		"postcss": "^8.5.6",
		"storybook": "^10.2.0",
		"tailwindcss": "^4.1.18",
		"tsx": "^4.19.2",
		"typescript": "^5.7.3",
		"vite": "^7.3.1"
	}
