{
	"name": "@pubint/yaboujee",
	"version": "0.0.0",
	"type": "module",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": "./src/index.ts",
		"./styles": "./src/index.css"
	},
	"scripts": {
		"build": "tsc",
		"clean": "rm -rf dist storybook-static",
		"typecheck": "tsc --noEmit",
		"test": "pnpm test:unit && pnpm test:interaction",
		"test:unit": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:interaction": "vitest run --config=vitest.storybook.config.ts && tsx ../../scripts/format-interaction-test-results.ts test-results/interaction-tests.json test-results/interaction-summary.md || tsx ../../scripts/format-interaction-test-results.ts test-results/interaction-tests.json test-results/interaction-summary.md",
		"test:interaction:watch": "vitest --config=vitest.storybook.config.ts --watch",
		"storybook": "storybook dev -p 6007",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"@base-ui/react": "^1.1.0",
		"@hello-pangea/dnd": "^18.0.1",
		"@pubint/storybook-config": "workspace:*",
		"@pubint/yabasic": "workspace:*",
		"@tanstack/react-form": "^1.28.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-react": "^0.563.0",
		"next": "^15.5.9",
		"pdfjs-dist": "^4.10.38",
		"react-rnd": "^10.5.2",
		"tailwind-merge": "^3.4.0"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^5.0.0",
		"@pubint/vitest-config": "workspace:*",
		"@storybook/addon-a11y": "^10.2.0",
		"@storybook/addon-docs": "^10.2.0",
		"@storybook/addon-vitest": "^10.2.0",
		"@storybook/react": "^10.2.0",
		"@storybook/react-vite": "^10.2.0",
		"@storybook/test": "^8.6.15",
		"@tailwindcss/vite": "^4.1.18",
		"@types/react": "^19.2.9",
		"@types/react-dom": "^19.2.3",
		"@vitest/browser-playwright": "^4.0.18",
		"playwright": "^1.58.0",
		"react": "^19.2.0",
		"react-dom": "^19.2.0",
		"storybook": "^10.2.0",
		"storybook-addon-pseudo-states": "^10.2.3",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.7.3",
		"vite": "^7.3.1",
		"vitest": "^4.0.18"
	},
	"peerDependencies": {
		"react": "^19.2.0",
		"react-dom": "^19.2.0"
	}
}
